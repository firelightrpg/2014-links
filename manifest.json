{
    "manifest_version": 3,
    "name": "D&D Beyond 2014 Link Fixer",
    "version": "1.0",
    "description": "Redirects links for conditions, skills, and senses to the 2014 D&D Basic Rules.",
    "permissions": [],
    "content_scripts": [
      {
        "matches": ["*://www.dndbeyond.com/*"],
        "js": ["content.js"],
        "run_at": "document_idle"
      }
    ],
    "icons": {
      "48": "icon.png"
    }
  }
  